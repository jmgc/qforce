    "version": "0.2.0",
    "configurations": [


        {
            "name": "Python Debugger: Current File with Arguments",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": "${command:pickArgs}",
            "cwd": "${workspaceFolder}/water_test"
        },
        {
            "name": "QForce",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/bin/qforce",
            "console": "integratedTerminal",
            "args": "run water_qforce",
            "cwd": "${workspaceFolder}/water_test",
            "justMyCode": false
        }
    ]
}